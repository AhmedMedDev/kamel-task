        "/api/categories/{categoryId}/products": {
            "post": {
                "operationId": "71fcad552bb0eaba9fb191fd8d8dcab0",
                "parameters": [
                    {
                        "name": "categoryId",
                        "in": "path",
                        "description": "category ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "description": "attach product to category",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "products": {
                                        "type": "array",
                                        "required": true
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "product attached successfully"
                    }
                }
            }
        },
        "/api/categories/{categoryId}/products/{productId}": {
            "delete": {
                "operationId": "71fcad552bb0eaba9fb191fd8d8dcab0",
                "parameters": [
                    {
                        "name": "categoryId",
                        "in": "path",
                        "description": "category ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "productId",
                        "in": "path",
                        "description": "product ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "product detached successfully"
                    }
                }
            }
        }
